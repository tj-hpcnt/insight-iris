{
  "name": "backend",
  "version": "0.0.1",
  "description": "NestJS backend for Insight Iris",
  "author": "",
  "private": true,
  "license": "Private",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-data": "ts-node scripts/generateInitialData.ts",
    "export-inspiration-data": "ts-node scripts/exportInspirationData.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "embed-insights": "python3 scripts/embedding/embed_insights.py --data_dir $(ls -dt src/generated/embedding/* | head -1)",
    "embed-insights-text": "python3 scripts/embedding/embed_text_insights.py --data_dir $(ls -dt src/generated/embedding/* | head -1)",
    "sample-random-profiles": "python3 scripts/embedding/score_random_profiles.py --data_dir $(ls -dt src/generated/embedding/* | head -1) --num_pairs=10 --profile_size=25",
    "explain-dimensions": "python3 scripts/embedding/explain_dimensions.py --data_dir $(ls -dt src/generated/embedding/* | head -1)"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@prisma/client": "^6.8.2",
    "dotenv": "^16.5.0",
    "openai": "^4.103.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
